





import numpy as np
import time as t

arr1 = np.random.randn(5000, 5000)
arr2 = np.random.randn(5000, 5000)

start = t.time()
arr3 = arr1 + arr2
print("Numpy's sum:", t.time() - start)

start = t.time()
arr4 = np.zeros_like(arr1)
for i in range(5000):
    for j in range(5000):
        arr4[i, j] = arr1[i, j] + arr2[i, j]
print("Python's sum:", t.time() - start)





